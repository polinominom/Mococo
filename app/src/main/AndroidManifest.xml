<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.polinominom.mococo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".OptionActivity"
            android:label="Option"
            android:theme="@style/AppTheme"/>

        <activity android:name=".PlayActivity"
            android:label="Play"
            android:theme="@style/AppTheme"/>

        <activity android:name=".AboutActivity"
            android:label="About"
            android:theme="@style/AppTheme"/>

        <activity android:name=".HowToPlayActivity"
            android:label="How To Play"
            android:theme="@style/AppTheme"/>

        <activity android:name=".ProfileActivity"
            android:label="Profile"
            android:theme="@style/AppTheme"/>

        <activity android:name=".DungeonActivity"
            android:label="Dungeon"
            android:theme="@style/AppTheme"/>

        <activity android:name=".TownActivity"
            android:label="Town"
            android:theme="@style/AppTheme"/>

        <activity android:name=".ShopActivity"
            android:label="Town"
            android:theme="@style/AppTheme"/>

        <activity android:name=".QuestActivity"
            android:label="Town"
            android:theme="@style/AppTheme"/>

        <activity android:name=".UpgradeActivity"
            android:label="Town"
            android:theme="@style/AppTheme"/>


    </application>

</manifest>
